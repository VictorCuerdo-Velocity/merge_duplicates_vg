{
  "version": 2,
  "public": true,
  "git": {
    "deploymentEnabled": true
  },
  "builds": [
    {
      "src": "reports/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/reports/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}