{
  "version": 2,
  "public": true,
  "git": {
    "deploymentEnabled": true
  },
  "builds": [
    {
      "src": "reports/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/reports/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "ignoreCommand": true
}